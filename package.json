{
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@symfony/webpack-encore": "^4.7.0",
    "compass-mixins": "^0.12.12",
    "jest": "^29.7.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.78.0",
    "sass-loader": "^14.2.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-notifier": "^1.15.0",
    "yarn-upgrade-all": "^0.7.4"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/js/**/*.test.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"],
    "collectCoverageFrom": [
      "tests/js/**/*.js",
      "!tests/js/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 60,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "packageManager": "yarn@3.2.4"
}
