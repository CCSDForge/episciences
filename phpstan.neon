parameters:
	level: 6
	paths:
		- application
		- library
		- public
		- scripts
	excludePaths:
		- cache
		- data
		- log
		- logs
		- tmp
		- vendor
	ignoreErrors:
		- '#Call to an undefined method Zend_.*#'
		- '#Access to an undefined property Zend_.*#'
	bootstrapFiles:
		- phpstan-bootstrap.php