{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":dependencyDashboard",
    ":semanticPrefixFixDepsChoreOthers",
    ":ignoreModulesAndTests",
    "group:monorepos",
    "group:recommended",
    "replacements:all",
    "workarounds:all"
  ],
  "baseBranches": ["staging"],
  "packageRules": [
    {
      "matchBaseBranches": ["staging"],
      "labels": ["staging-update"]
    }
  ]
}